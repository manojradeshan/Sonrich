/*-----------------------------------------------Banner Section-----------------------------------------------------------*/
.banner-section .container-fluid {
    padding-left: 0px;
    padding-right: 0px;
}

.banner-section .container-fluid .background-image {
    padding-left: 0px;
    padding-right: 0px;
    position: relative;
}

.banner-section .background-image {
    background-image: url(./inc/Images/sonrich-banner.jpg);
    min-height: 600px;
    height: auto;
    position: relative;
    width: 100%;
    background-size: cover;
    background-repeat: no-repeat;
    /* position: relative; */
}

.banner-content {
    top: 300px;
    position: absolute;
    left: 115px;
}

h6.banner-slagon {
    font-size: 27px;
    font-weight: 300;
    color: #fff;
    letter-spacing: 2px;
    text-shadow: 2px 2px 5px #000000;
    padding-left: 6px;

}

h5.banner-heading {
    font-size: 80px;
    color: #1f717f;
}

